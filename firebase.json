{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/**",
        "destination": "/default.html"
      },
      {
        "source": "/**/*",
        "destination": "/default.html"
      }
    ]
  }
}
